# A sample Gemfile
source "https://rubygems.org"

gem 'sinatra'
gem 'thin'
gem 'require_all'
gem 'activerecord', '5.2'
gem 'sinatra-activerecord'
gem 'rake'


group :development do #none of these are required in production, theyre just useful for developers
	gem 'shotgun'
	gem 'pry'
	gem 'tux'
	gem 'sqlite3', '~> 1.3.6'
end
